# Execution System Environment Configuration

# Server Configuration
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# Main API Configuration
MAIN_API_URL=http://localhost:3001
MAIN_API_PORT=3001

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# Docker Configuration
DOCKER_HOST=unix:///var/run/docker.sock
DOCKER_NETWORK=none

# Execution Limits
EXECUTION_TIMEOUT=5000
MEMORY_LIMIT=128m
CPU_LIMIT=0.5
PIDS_LIMIT=64

# Temporary Files
TEMP_DIR=./temp
HOST_TEMP_DIR=./temp
CLEANUP_INTERVAL=300000

# Security Configuration
USER_ID=1000
GROUP_ID=1000
SECURITY_OPTS=no-new-privileges

# Supported Languages
SUPPORTED_LANGUAGES=python,javascript,typescript,java,cpp,c

# Debug Settings
DEBUG_MODE=false
LOG_LEVEL=info
VERBOSE_LOGGING=false